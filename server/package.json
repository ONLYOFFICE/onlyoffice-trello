{
  "name": "onlyoffice-trello-server",
  "version": "1.0.0",
  "author": "Ascensio System SIA",
  "license": "Apache-2.0",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "env-cmd -f .env jest",
    "test:watch": "env-cmd -f .env jest --watch",
    "test:cov": "env-cmd -f .env jest --coverage",
    "test:e2e": "env-cmd -f .env jest --config ./test/jest-e2e.json",
    "doc:generate": "npx typedoc --entryPointStrategy expand ./src"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.1.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/throttler": "^2.0.0",
    "axios": "^0.28.0",
    "cache-manager": "^3.4.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "express-useragent": "^1.0.15",
    "form-data": "^3.0.1",
    "hbs": "^4.1.2",
    "helmet": "^4.6.0",
    "joi": "^17.5.0",
    "jsonwebtoken": "^8.5.1",
    "mime-types": "^2.1.34",
    "nest-winston": "^1.6.2",
    "oauth-1.0a": "^2.2.6",
    "prom-client": "^14.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "winston": "^3.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.2.5",
    "@types/cache-manager": "^3.4.2",
    "@types/express": "^4.17.13",
    "@types/express-useragent": "^1.0.2",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-promise": "^5.2.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typedoc": "^0.22.8",
    "typescript": "^4.3.5"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "es6": true,
      "browser": true
    },
    "rules": {
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-unused-vars": [
        "warn"
      ],
      "no-var": [
        "off"
      ],
      "one-var": [
        "off"
      ]
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@controllers/(.*)": "<rootDir>/controllers/$1",
      "^@services/(.*)": "<rootDir>/services/$1",
      "^@utils/(.*)": "<rootDir>/utils/$1",
      "^@models/(.*)": "<rootDir>/models/$1",
      "^@guards/(.*)": "<rootDir>/guards/$1"
    }
  },
  "_moduleAliases": {
    "@controllers": "dist/controllers",
    "@services": "dist/services",
    "@models": "dist/models",
    "@utils": "dist/utils",
    "@modules": "dist/modules",
    "@middlewares": "dist/middlewares",
    "@filters": "dist/filters",
    "@guards": "dist/guards"
  }
}
